<odoo>
    <record id="waran_diagnosis_view_tree" model="ir.ui.view">
        <field name="name">waran.diagnosis.tree</field>
        <field name="model">waran.diagnosis</field>
        <field name="arch" type="xml">
            <tree>
                <field name="code"/>
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="waran_diagnosis_view_form" model="ir.ui.view">
        <field name="name">waran.diagnosis.form</field>
        <field name="model">waran.diagnosis</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="code"/>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="waran_diagnosis_action" model="ir.actions.act_window">
        <field name="name">Diagnoses</field>
        <field name="res_model">waran.diagnosis</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
