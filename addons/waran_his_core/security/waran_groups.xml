<odoo>
    <data noupdate="1">
        <record id="group_waran_registration" model="res.groups">
            <field name="name">Registration</field>
        </record>
        <record id="group_waran_nurse" model="res.groups">
            <field name="name">Nurse</field>
        </record>
        <record id="group_waran_doctor" model="res.groups">
            <field name="name">Doctor</field>
        </record>
        <record id="group_waran_labtech" model="res.groups">
            <field name="name">LabTech</field>
        </record>
        <record id="group_waran_pharmacist" model="res.groups">
            <field name="name">Pharmacist</field>
        </record>
        <record id="group_waran_cashier" model="res.groups">
            <field name="name">Cashier</field>
        </record>
        <record id="group_waran_admin" model="res.groups">
            <field name="name">Admin</field>
            <field name="category_id" ref="base.module_category_administration"/>
        </record>
        <record id="patient_rule_registration" model="ir.rule">
            <field name="name">Patient registration rule</field>
            <field name="model_id" ref="model_waran_patient"/>
            <field name="groups" eval="[(4, ref('group_waran_registration'))]"/>
            <field name="domain_force">[(1,'=',1)]</field>
        </record>
    </data>
</odoo>
