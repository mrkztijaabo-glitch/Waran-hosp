<odoo>
    <record id="waran_lab_order_view_tree" model="ir.ui.view">
        <field name="name">waran.lab_order.tree</field>
        <field name="model">waran.lab_order</field>
        <field name="arch" type="xml">
            <tree>
                <field name="patient_id"/>
                <field name="test_type"/>
                <field name="priority"/>
                <field name="state"/>
            </tree>
        </field>
    </record>
    <record id="waran_lab_order_view_form" model="ir.ui.view">
        <field name="name">waran.lab_order.form</field>
        <field name="model">waran.lab_order</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="patient_id"/>
                        <field name="test_type"/>
                        <field name="priority"/>
                        <field name="specimen"/>
                        <field name="status"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="waran_lab_order_action" model="ir.actions.act_window">
        <field name="name">Lab Orders</field>
        <field name="res_model">waran.lab_order</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
