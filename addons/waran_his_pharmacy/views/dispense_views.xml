<odoo>
    <record id="waran_dispense_view_tree" model="ir.ui.view">
        <field name="name">waran.dispense.tree</field>
        <field name="model">waran.dispense</field>
        <field name="arch" type="xml">
            <tree>
                <field name="prescription_id"/>
                <field name="dispensed_by"/>
                <field name="date"/>
                <field name="qty"/>
            </tree>
        </field>
    </record>
    <record id="waran_dispense_view_form" model="ir.ui.view">
        <field name="name">waran.dispense.form</field>
        <field name="model">waran.dispense</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="prescription_id"/>
                        <field name="dispensed_by"/>
                        <field name="date"/>
                        <field name="qty"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="waran_dispense_action" model="ir.actions.act_window">
        <field name="name">Dispense</field>
        <field name="res_model">waran.dispense</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
